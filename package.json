{
	"name": "atvluatest",
	"displayName": "AtvLuaTest",
	"description": "ATV Lua test script development",
	"publisher": "ATV GmbH Torsten Brischalle",
	"version": "1.0.0",
	"engines": {
		"vscode": "^1.58.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:atvluatest.start"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "atvluatest.start",
				"title": "ATV Lua test: Start"
			},
			{
				"command": "atvluatest.connect",
				"title": "ATV Lua test: Connect"
			},
			{
				"command": "atvluatest.disconnect",
				"title": "ATV Lua test: Disconnect"
			},
			{
				"command": "atvluatest.changeDeviceAddress",
				"title": "ATV Lua test: Change device address"
			},
			{
				"command": "atvluatest.execute",
				"title": "ATV Lua test: Execute"
			},
			{
				"command": "atvluatest.printErrors",
				"title": "ATV Lua test: Print errors"
			}
		],
		"keybindings": [
			{
				"command": "atvluatest.start",
				"key": "ctrl+alt+s"
			},
			{
				"command": "atvluatest.execute",
				"key": "ctrl+alt+e"
			},
			{
				"command": "atvluatest.printErrors",
				"key": "ctrl+alt+p"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.58.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.2",
		"@types/node": "14.x",
		"eslint": "^7.27.0",
		"@typescript-eslint/eslint-plugin": "^4.26.0",
		"@typescript-eslint/parser": "^4.26.0",
		"glob": "^7.1.7",
		"mocha": "^8.4.0",
		"typescript": "^4.3.2",
		"vscode-test": "^1.5.2"
	},
	"dependencies": {
		"vsce": "^1.95.1"
	}
}
